{
	"components": {
		"Start Tomcat Script": {
			"window": {
				"id": "Tomcat",
				"windowType": "assimilation",
				"path": "$tomcatPath/bin/catalina.bat",
				"url": "",
				"arguments": "start",
				"addToWorkspace": false
			},
			"component": {
				"spawnOnStartup": false
			},
			"foreign": {
				"services": {
					"workspaceService": {
						"isArrangable": true
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": true
					},
					"Window Manager": {
						"persistWindowState": false,
						"FSBLHeader": true
					},
					"Toolbar": {
						"iconURL": "$applicationRoot/assets/img/notepad.png"
					}
				}
			}
		},
		"Stop Tomcat Script": {
			"window": {
				"id": "Tomcat",
